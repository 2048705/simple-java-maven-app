pipeline {
        agent any
        tools {
        // Use the correct SonarQube installation name
        sonarqube 'SonarQubeScanner'
    }
        stages {
         
          stage("build & SonarQube Scanner") {
            agent any
            steps {
              withSonarQubeEnv('SonarQube Scanner') {
                sh 'mvn clean package sonar:sonar'
              }
            }
          }
        }
      }
